{
  "name": "ChatGPT Emissions Tracker",
  "description": "",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_title": "TODO popup",
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "32": "assets/icons/icon32.png",
      "64": "assets/icons/icon64.png",
      "128": "assets/icons/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "64": "assets/icons/icon64.png",
    "128": "assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.js-loader-lX0mXYNC.js"
      ],
      "run_at": "document_idle",
      "matches": [
        "https://chatgpt.com/*"
      ],
      "css": [
        "assets/storage-FapxwYOU.css"
      ]
    }
  ],
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://chatgpt.com/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "resources": [
        "assets/storage-CXpF4OEx.js",
        "assets/content.js-Cst0cmY3.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
